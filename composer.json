{
    "name": "setono/sylius-stock-movement-plugin",
    "type": "sylius-plugin",
    "description": "Plugin to handle stock related activities",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "kucharovic/money-bundle": "^1.1",
        "league/flysystem-bundle": "^1.1",
        "moneyphp/money": "^3.2",
        "setono/cron-expression-bundle": "^1.0.0",
        "sylius/resource-bundle": "^1.6@beta",
        "sylius/sylius": "^1.3",
        "symfony/event-dispatcher": "^4.3",
        "symfony/messenger": "^4.3",
        "thecodingmachine/safe": "^0.1.15"
    },
    "conflict": {
        "symfony/browser-kit": "4.1.8",
        "symfony/dependency-injection": "4.1.8",
        "symfony/dom-crawler": "4.1.8",
        "symfony/routing": "4.1.8",
        "symfony/symfony": "4.1.8"
    },
    "require-dev": {
        "lchrusciel/api-test-case": "^4.1",
        "localheinz/composer-normalize": "^1.2",
        "phpspec/phpspec": "^5.0",
        "phpstan/phpstan-doctrine": "^0.11",
        "phpstan/phpstan-shim": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11.1",
        "phpstan/phpstan-symfony": "^0.11",
        "phpstan/phpstan-webmozart-assert": "^0.11",
        "phpunit/phpunit": "^8.2",
        "roave/security-advisories": "dev-master",
        "sensiolabs/security-checker": "^5.0",
        "sylius-labs/coding-standard": "^3.0",
        "symfony/browser-kit": "^3.4 || ^4.1",
        "symfony/debug-bundle": "^3.4 || ^4.1",
        "symfony/dotenv": "^4.2",
        "symfony/intl": "^3.4 || ^4.1",
        "symfony/web-profiler-bundle": "^3.4 || ^4.1",
        "symfony/web-server-bundle": "^3.4 || ^4.1",
        "thecodingmachine/phpstan-safe-rule": "^0.1.3"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Setono\\SyliusStockMovementPlugin\\": "src/",
            "Tests\\Setono\\SyliusStockMovementPlugin\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "analyse": [
            "@ensure-test-container-exists",
            "./vendor/bin/phpstan analyse -c phpstan.neon -l max src"
        ],
        "check-style": "vendor/bin/ecs check --ansi spec src",
        "ensure-test-container-exists": "[[ -f tests/Application/var/cache/test/ApplicationTests_Setono_SyliusStockMovementPlugin_Application_KernelTestDebugContainer.xml ]] || (cd tests/Application && php -d memory_limit=-1 bin/console cache:clear --env=test)",
        "fix-style": "vendor/bin/ecs check --ansi spec src --fix",
        "phpspec": "vendor/bin/phpspec run --ansi",
        "phpunit": "vendor/bin/phpunit",
        "test": [
            "@phpunit",
            "@phpspec"
        ]
    }
}
