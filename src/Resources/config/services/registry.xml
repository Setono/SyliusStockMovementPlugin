<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="setono_sylius_stock_movement.registry.template" class="Setono\SyliusStockMovementPlugin\Registry\TemplateRegistry" />

        <service id="setono_sylius_stock_movement.registry.data_source" class="Sylius\Component\Registry\ServiceRegistry">
            <argument>Setono\SyliusStockMovementPlugin\DataSource\DataSourceInterface</argument>
            <argument>data sources</argument>
        </service>

        <service id="setono_sylius_stock_movement.registry.transport" class="Sylius\Component\Registry\ServiceRegistry">
            <argument>Setono\SyliusStockMovementPlugin\Transport\TransportInterface</argument>
            <argument>transport</argument>
        </service>
        <service id="setono_sylius_stock_movement.form_registry.transport" class="Sylius\Bundle\ResourceBundle\Form\Registry\FormTypeRegistry" />
    </services>
</container>
